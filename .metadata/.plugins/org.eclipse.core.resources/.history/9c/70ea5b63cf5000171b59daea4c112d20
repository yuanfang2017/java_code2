package swing.cn;

import java.awt.Color;

import javax.swing.JFrame;

/*
 * 新建一个窗口
 */
public class Test {
	public static void main(String[] args) {
       JFrame jf = new JFrame("第一个swing窗体");
        jf.setSize(230, 80);
        jf.setBackground(Color.white);
        jf.setLocation(300, 200);
        jf.setVisible(true);
	}

}
